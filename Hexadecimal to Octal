import java.util.Scanner;

public class Main {
  public static void main(String[] args){

  Scanner input = new Scanner(System.in);

  System.out.print("Hexadecimal Input: ");

  String lastInput = input.next();

  int changedInputToInt = Integer.parseInt(lastInput, 16);

  System.out.println("Octal Output: " + Integer.toOctalString(changedInputToInt));
  }
}
