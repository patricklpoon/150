import java.util.Scanner;

public class Main{
public static void main (String[] args) {

  Scanner input = new Scanner(System.in);
  System.out.println("Input: ");
  String extractInput = input.next();
  int inputToInt = Integer.parseInt(extractInput, 2);
  System.out.println("Output:" + Integer.toOctalString(inputToInt));

  }
}
